{
    "version": 2,
    "builds": [
      { "src": "api/**/*.js", "use": "@vercel/node" },
      { "src": "src/index.js", "use": "@vercel/static-build", "config": { "distDir": "build" } }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/src/index.html" }
    ]
  }
  